{
    "routes": [
        {
            "path": "/admin/taxonomy",
            "controller": "ride\\web\\orm\\controller\\TaxonomyController",
            "action": "vocabulariesAction",
            "id": "taxonomy.vocabulary.list"
        },
        {
            "path": "/admin/taxonomy/add",
            "controller": "ride\\web\\orm\\controller\\TaxonomyController",
            "action": "vocabularyFormAction",
            "id": "taxonomy.vocabulary.add"
        },
        {
            "path": "/admin/taxonomy/%vocabulary%/edit",
            "controller": "ride\\web\\orm\\controller\\TaxonomyController",
            "action": "vocabularyFormAction",
            "id": "taxonomy.vocabulary.edit"
        },
        {
